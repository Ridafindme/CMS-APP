{
  "expo": {
    "name": "CMS",
    "slug": "cms",
    "scheme": "com.cms.app",
    "version": "0.3.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cms.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2563EB"
      },
      "package": "com.cms.app",
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.cms.app"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBxKysxV5MNeukbU2rmXEwmu-9T7TGPULo"
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CMS to use your location to find nearby clinics."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow CMS to access your photos to set your profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [],
          "enableAutoStart": false,
          "icon": "./assets/icon.png",
          "color": "#2563EB"
        }
      ],
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "071beeff-64f2-4989-8fdf-f7e478d74765"
      }
    },
    "owner": "ridaexpo"
  }
}
